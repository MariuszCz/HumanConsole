<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="148.0" prefWidth="502.0" xmlns="http://javafx.com/javafx/8.0.112-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
    <children>
        <Button fx:id="submitButton" layoutX="368.0" layoutY="60.0" mnemonicParsing="false" onAction="#runProgram" prefHeight="31.0" prefWidth="90.0" text="Ok" />
        <TextField fx:id="programCommand" layoutX="55.0" layoutY="60.0" prefHeight="31.0" prefWidth="262.0" />
      <Button fx:id="speechButton" layoutX="200.0" layoutY="103.0" mnemonicParsing="false" onAction="#startSpeech" text="Start speech" />
      <Button onAction="#stopSpeech" layoutX="348.0" layoutY="103.0" mnemonicParsing="false" text="Stop speech" />
    </children>
</AnchorPane>
